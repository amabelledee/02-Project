<body class="bg-cover">      
    <div class="container m-3">
        <div class="row">
            <div>
                <div class="m-5">
                    <h5 class="text-uppercase m-2"><strong>Upcoming Events:</strong></h5>
                    <ul id="example-list" class="list-group">
                        {{#each examples}}
                        <li data-id="{{this.id}}" class="list-group-item">
                            <div><a href="example/{{this.id}}" class="link">{{this.title}}</a></div>
                            <div><strong>Event Venue:</strong> {{this.venue}}</div>
                            <div><strong>Event Location:</strong> {{this.venueaddress}}</div>
                            <div><strong>Event Date &amp; Time:</strong> {{this.eventDate}}</div>
                            <div><strong>Vendor:</strong> {{this.vendor}}</div>
                            <div><strong>Vendor Email:</strong> {{this.vendoremail}}</div>
                        </li>
                        {{/each}}
                    </ul>
                    <div><a href="/dashboard">‚Üê Back To Dasboard</a></div>
                </div>
            </div>
        </div>
    </div>
</body>